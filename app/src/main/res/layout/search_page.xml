                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/outdoor"
                            android:layout_width="88.5dp"
                            android:layout_height="23.5dp"
                            android:clickable="true"
                            android:fontFamily="Montserrat"
                            android:text="@string/outdoor_string"
                            android:textColor="@color/penang_hill_color"
                            android:textSize="15sp" />
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
