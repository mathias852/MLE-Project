                            android:src="@drawable/bookmark_icon" />

                        <ImageView
                            android:id="@+id/bookmarkIconGurneyDriveBookmarked"
                            android:layout_width="18.75dp"
                            android:layout_height="25dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="10dp"
