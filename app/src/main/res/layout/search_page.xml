                            android:layout_height="23.5dp"
                            android:layout_marginStart="20dp"
                            android:clickable="true"
                            android:fontFamily="Montserrat"
                            android:text="@string/all_string"
                            android:textColor="@color/penang_hill_color"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/restaurants"
                            android:layout_width="112.5dp"
                            android:layout_height="23.5dp"
                            android:clickable="true"
                            android:fontFamily="Montserrat"
                            android:text="@string/restaurants_string"
                            android:textColor="@color/penang_hill_color"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/party"
                            android:layout_width="62.5dp"
                            android:layout_height="23.5dp"
                            android:clickable="true"
                            android:fontFamily="Montserrat"
                            android:text="@string/party_string"
                            android:textColor="@color/penang_hill_color"
                            android:textSize="15sp" />

                        <TextView
