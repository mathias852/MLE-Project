                            android:visibility="visible"
                            android:src="@drawable/bookmark_icon" />

                        <ImageView
                            android:id="@+id/bookmarkIconPenangHillBookmarked"
                            android:layout_width="18.75dp"
                            android:layout_height="25dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="10dp"
                            android:contentDescription="@string/penang_hill"
                            android:onClick="updateBookmark"
                            android:src="@drawable/bookmark_icon_selected"
                            android:visibility="invisible" />

                        <TextView
                            android:id="@+id/penang_hill"
