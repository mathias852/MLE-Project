                        android:layout_width="152dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="215dp"
                        android:layout_marginTop="337dp">

                        <ImageView
                            android:id="@+id/rectangle_5_ek1"
                            android:layout_width="match_parent"
                            android:layout_height="144dp"
                            android:onClick="sorryNotSupported"
                            android:src="@drawable/bookmark2_img" />

                        <ImageView
                            android:id="@+id/bookmarkIconKekLokSiTemple"
                            android:layout_width="18.75dp"
                            android:layout_height="25dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="10dp"
                            android:onClick="updateBookmark"
                            android:contentDescription="@string/kek_lok_si_temple_string"
                            android:visibility="visible"
                            android:src="@drawable/bookmark_icon" />

                        <ImageView
                            android:id="@+id/bookmarkIconKekLokSiTempleBookmarked"
                            android:layout_width="18.75dp"
                            android:layout_height="25dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="10dp"
                            android:onClick="updateBookmark"
                            android:contentDescription="@string/kek_lok_si_temple_string"
